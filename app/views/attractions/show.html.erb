<% if !@user.admin%>
  <form action="/users/<%=session[:user_id]%>" method="get">
    <input type="hidden" name="attraction" value="<%=@attraction.id%>">
  <button type="submit" name="button">Go on this ride</button>
  </form>
<%else%>
  <%= link_to "Edit Attraction", attraction_edit_path(@attraction.id) %>
<%end%>
<%= @attraction.name %>
