<%= form_for @attraction  do |f| %>
  <%= f.text_field :name, placeholder: 'name' %><br><br>
  <%= f.number_field :tickets, placeholder: 'tickets' %><br><br>
  <%= f.number_field :nausea_rating, placeholder: 'nausea' %><br><br>
  <%= f.number_field :happiness_rating, placeholder: 'happiness' %><br><br>
  <%= f.number_field :min_height, placeholder: 'min_height' %><br><br>
  <%= f.submit %>
<% end %>