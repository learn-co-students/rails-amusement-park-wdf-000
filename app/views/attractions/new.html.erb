<%= form_for @attraction do |f| %>
<%= f.label :name, class: "control-label" %>
<%= f.text_field :name, class: "form-control" %>
<br>
<%= f.label :min_height, class: "control-label" %>
<%= f.text_field :min_height, class: "form-control" %>
<br>
<%= f.label :nausea_rating, class: "control-label" %>
<%= f.text_field :nausea_rating, class: "form-control" %>
<br>
<%= f.label :happiness_rating, class: "control-label" %>
<%= f.text_field :happiness_rating, class: "form-control" %>
<br>
<%= f.label :tickets, class: "control-label" %>
<%= f.text_field :tickets, class: "form-control" %>
<br>
<%= f.submit "Create Attraction", class: "btn btn-primary" %>
<% end %>
