<% if flash[:notice] %>
  <div class="notice"><%= flash[:notice] %></div>
<% end %>

<% if @user.admin %>
  <p>ADMIN</p>
<% end %>

<p>Name: <%= @user.name %></p>
<p>Height: <%= @user.height %></p>
<p>Mood: <%= @user.mood %></p>
<p>Tickets: <%= @user.tickets %></p>

<% if session[:user_id] == @user.id %>
  <p><%= link_to 'Edit', edit_user_path(@user) %></p>
<% end %>
<p>
<%= link_to 'See attractions', attractions_path %>
</p>